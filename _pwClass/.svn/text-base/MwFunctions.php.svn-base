<?php
function autoLoader($classname) {
	$fname = $classname.".Class.php";
	if(file_exists("../mwClass/".$fname)){
		require_once "../mwClass/".$fname;
		return true;
	}
	elseif(file_exists("../mwModels/".$fname)){
		require_once "../mwModels/".$fname;
		return true;
	}
	$fname = $classname.".Interface.php";
	if(file_exists("../mwClass/".$fname)){
		require_once "../mwClass/".$fname;
		return true;
	}
	else {
		MwDebug::debug("ClassName",$classname."- "." <".$classname."> - <".$fname.">");
		MwDebug::debugInLogFile("autoLoad Erreur chargement de class : <".$classname."> - <".$fname.">");
	}
}	
	
	
	
	
	
